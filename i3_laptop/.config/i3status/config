general {
    colors = false
    interval =5
}

order += "wireless wlp4s0"
order += "battery 0"
order += "tztime local"

wireless wlp4s0 {
    format_up = " %essid "
    format_down = "disconnected"
}

battery 0 {
    format = " %status %percentage %emptytime "
}

tztime local {
    format = " %A %B %d, %H:%M:%S"
}
